<ul>
  <% @users.each do |user| %>
    <li class='card'>
      <%= user.name %>
      <%= user.email %>
      <div class="buttons">
        <%= link_to 'Edit User', "/admin/users/#{user.id}/edit" %>
        <%= button_to 'Delete User', admin_user_path(user), method: 'delete' %>
      </div>
    </li>
  <% end %>
</ul>
