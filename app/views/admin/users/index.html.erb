<%= link_to 'Create a New User', new_admin_user_path %>

<ul>
  <% @users.each do |user| %>
    <li class='card'>
      <%= user.name %>
      <%= user.email %>
      <div class="buttons">
        <%= link_to 'Edit User', edit_admin_user_path(user) %>
        <%= button_to 'Delete User', admin_user_path(user), method: 'delete' %>
      </div>
    </li>
  <% end %>
</ul>
